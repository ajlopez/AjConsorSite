<Entity>
	<Name>UsoMultiple</Name>
	<Description>Salon de Uso Multiple</Description>
	<SetName>UsoMultiples</SetName>
	<Descriptor>UsoMultiple</Descriptor>
	<SetDescriptor>UsoMultiples</SetDescriptor>
	<SqlTable>usomultiples</SqlTable>
	
	<Properties>

		<Property>
			<Name>Id</Name>
			<Type>Id</Type>
		</Property>

		<Property>
			<Name>Nombre</Name>
			<Type>Text</Type>
			<SqlType>varchar(200)</SqlType>
		</Property>

		<Property>
			<Name>Codigo</Name>
			<Type>Text</Type>
			<SqlType>varchar(10)</SqlType>
		</Property>

		<Property>
			<Name>IdConsorcio</Name>
			<Type>IdRef</Type>
			<SqlType>int</SqlType>
			<Reference>Consorcio</Reference>
		</Property>

		<Property>
			<Name>Notas</Name>
			<Type>Memo</Type>
			<SqlType>text</SqlType>
		</Property>
		
	</Properties>
</Entity>
