<Entity>
	<Name>Unidad</Name>
	<Description>Unidad</Description>
	<SetName>Unidades</SetName>
	<Descriptor>Unidad</Descriptor>
	<SetDescriptor>Unidades</SetDescriptor>
	<SqlTable>unidades</SqlTable>
	
	<Properties>

		<Property>
			<Name>Id</Name>
			<Type>Id</Type>
		</Property>

		<Property>
			<Name>Nombre</Name>
			<Type>Text</Type>
			<SqlType>varchar(200)</SqlType>
		</Property>

		<Property>
			<Name>Piso</Name>
			<Type>Text</Type>
			<SqlType>varchar(20)</SqlType>
		</Property>

		<Property>
			<Name>Numero</Name>
			<Description>Nro/Letra</Description>
			<Type>Text</Type>
			<SqlType>varchar(20)</SqlType>
		</Property>

		<Property>
			<Name>IdConsorcio</Name>
			<Type>IdRef</Type>
			<SqlType>int</SqlType>
			<Reference>Consorcio</Reference>
		</Property>

		<Property>
			<Name>Notas</Name>
			<Type>Memo</Type>
			<SqlType>text</SqlType>
		</Property>
		
	</Properties>
</Entity>
